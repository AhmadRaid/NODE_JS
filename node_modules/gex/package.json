  ],
  "license": "MIT",
  "dependencies": {
    "underscore": "~1.6.0"
  },
  "main": "gex.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjrodger/gex.git"
  },
  "files": [
    "README.md",
    "LICENSE.txt",
    "gex.js",
    "gex-min.js",
    "gex-min.map"
  ],
  "scripts": {
